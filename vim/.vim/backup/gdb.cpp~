#include <iostream>
using namespace std;

class A {
public:
  A (int priv):m_priv(priv){};
  virtual ~A (){};
  int m_priv;
};


class B {
public:
  B (){};
  virtual ~B (){};
  
  void f(A& a)
  {
    a.m_priv++;
    std::cout << " Ez a B ";
  }
private:
};

int main(int argc, const char *argv[])
{
  A a(1);
  B b;
  b.f();
  return 0;
}
